            <property name="queryText">select 	CN.Serial_Number AS SERIAL_NUMBER, 
		CN.Expiry_Date AS EXPIRY_DATE, 
		CN.Status AS STATUS, 
		CN.Date_Created As DATE_CREATED, 
		CN.Date_Closed AS DATE_CLOSED, 
		AB.Account_Balance AS ACCOUNT_BALANCE,
		AB.Currency_Id AS CURRENCY_ID
from 	CreditNotes CN 
inner join AccountBalances AB on CN.Serial_Number = AB.Account_Id
where 	( CN.Expiry_Date IS NULL OR
            ((CN.Expiry_Date >= ?) AND (CN.Expiry_Date &lt;= ?)))
and 	(? = !~''~! or CN.Status = ?)
and 	(CN.Date_Created >= ?)
and 	(CN.Date_Created &lt;= ?)
and 	( CN.Date_Closed IS NULL OR
            ((CN.Date_Closed >= ?) and (CN.Date_Closed &lt;= ?)))
</property>
