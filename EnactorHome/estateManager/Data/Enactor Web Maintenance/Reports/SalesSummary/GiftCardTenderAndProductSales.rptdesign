            <list-property name="resultSet"/>
            <xml-property name="queryText"><![CDATA[
select
	COMPLETED_DATE,
	sum(TENDER_DEBIT_QUANTITY) as TENDER_DEBIT_QUANTITY,
	-sum(TENDER_DEBIT_VALUE) as TENDER_DEBIT_VALUE,
	sum(TENDER_CREDIT_QUANTITY) as TENDER_CREDIT_QUANTITY,
	sum(TENDER_CREDIT_VALUE) as TENDER_CREDIT_VALUE,
	sum(PRODUCT_SALES_QUANTITY) as PRODUCT_SALES_QUANTITY,
	sum(PRODUCT_SALES_VALUE) as PRODUCT_SALES_VALUE,
	sum(PRODUCT_RETURNS_QUANTITY) as PRODUCT_RETURNS_QUANTITY,
	sum(PRODUCT_RETURNS_VALUE) as PRODUCT_RETURNS_VALUE,
	sum(TENDER_DEBIT_QUANTITY)+sum(TENDER_CREDIT_QUANTITY)+sum(PRODUCT_SALES_QUANTITY)+sum(PRODUCT_RETURNS_QUANTITY) as TOTAL_QUANTITY,
	-sum(BASE_VALUE) as TOTAL_VALUE,
	CURRENCY_ID
from
	(select
		cast(RTI.Completed_Date_Time as Date) as COMPLETED_DATE,
		sum(TT.Base_Value) as BASE_VALUE,
		sum(case when TT.Value<0 then 1 else 0 end) as TENDER_DEBIT_QUANTITY,
		sum(case when TT.Value<0 then TT.Value else 0 end) as TENDER_DEBIT_VALUE,
		sum(case when TT.Value>0 then 1 else 0 end) as TENDER_CREDIT_QUANTITY,
		sum(case when TT.Value>0 then TT.Value else 0 end) as TENDER_CREDIT_VALUE,
		0 as PRODUCT_SALES_QUANTITY,
		0 as PRODUCT_SALES_VALUE,
		0 as PRODUCT_RETURNS_QUANTITY,
		0 as PRODUCT_RETURNS_VALUE,
		TT.Currency_Id as CURRENCY_ID
	from TenderTransactions TT
	INNER JOIN RetailTransactionIds RTI on RTI.Transaction_Key=TT.Transaction_Key
	where TT.Tender_Type = 'giftCardTender'
		and	(? = !~''~! or RTI.Region_Group_Id = ?) 
		and	(? = !~''~! or RTI.Region_Group_Hierarchy_Id = ?) 
		and	(? = !~''~! or RTI.Location_Id = ?) 
		and	(? = !~''~! or TT.User_Id = ?) 
		and	(? = !~''~! or RTI.Device_Id = ?) 
		and	(RTI.Completed_Date_Time >= ?)
		and	(RTI.Completed_Date_Time <= ?)
	group by
		cast(RTI.Completed_Date_Time as Date),
		TT.Tender_Type,
		TT.Currency_Id
	union
	select
		cast(RTI.Completed_Date_Time as Date) as COMPLETED_DATE,
		sum(NMS.Sales_Value) as BASE_VALUE,
		0 as TENDER_DEBIT_QUANTITY,
		0 as TENDER_DEBIT_VALUE,
		0 as TENDER_CREDIT_QUANTITY,
		0 as TENDER_CREDIT_VALUE,
		sum(case when NMS.Sales_Value>0 then 1 else 0 end) as PRODUCT_SALES_QUANTITY,
		sum(case when NMS.Sales_Value>0 then NMS.Sales_Value else 0 end) as PRODUCT_SALES_VALUE,
		sum(case when NMS.Sales_Value<0 then 1 else 0 end) as PRODUCT_RETURNS_QUANTITY,
		sum(case when NMS.Sales_Value<0 then NMS.Sales_Value else 0 end) as PRODUCT_RETURNS_VALUE,
		RTI.Base_Currency_Id as CURRENCY_ID
	from NonMerchandiseSales NMS
	INNER JOIN Products P on NMS.Product_Id = P.Product_Id
	INNER JOIN RetailTransactionIds RTI on RTI.Transaction_Key=NMS.Transaction_Key
	where p.Type = 'giftCardProduct'
		and	(? = !~''~! or RTI.Region_Group_Id = ?) 
		and	(? = !~''~! or RTI.Region_Group_Hierarchy_Id = ?) 
		and	(? = !~''~! or RTI.Location_Id = ?) 
		and	(? = !~''~! or NMS.User_Id = ?) 
		and	(? = !~''~! or RTI.Device_Id = ?) 
		and	(RTI.Completed_Date_Time >= ?)
		and	(RTI.Completed_Date_Time <= ?)
	group by
		cast(RTI.Completed_Date_Time as Date),
		P.Type,
		RTI.Base_Currency_Id) ttls
group by
	COMPLETED_DATE,
	CURRENCY_ID
order by
	COMPLETED_DATE
]]></xml-property>
