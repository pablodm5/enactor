            <property name="queryText">select	RTI.Completed_Year as COMPLETED_YEAR,
			RTI.Completed_Day as COMPLETED_DAY,
			RTI.Base_Currency_Id as BASE_CURRENCY_ID,
			MT.User_Id as USER_ID,
			U.Forename as USER_FORENAME,
			U.Surname as USER_SURNAME,
			COUNT( DISTINCT RTI.Transaction_Key ) as COUNT,
			sum( MT.Sales_Value ) + sum( MT.Orders_Value ) as SALES_VALUE
from 	MerchandiseTransactions MT
inner join RetailTransactionIds RTI on MT.Transaction_Key = RTI.Transaction_Key
left outer join Users U on U.User_Id = MT.User_Id
where (? = !~''~! or RTI.Location_Id = ?) 
and (? = !~''~! or RTI.Device_Id = ?) 
and (RTI.Completed_Date_Time >= ?)
and (RTI.Completed_Date_Time &lt;= ?)
group by RTI.Completed_Year, RTI.Completed_Day, RTI.Base_Currency_Id, MT.User_Id, U.Forename, U.Surname
order by RTI.Completed_Year, RTI.Completed_Day, MT.User_Id, U.Forename, U.Surname</property>
