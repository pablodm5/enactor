    <list-property name="propertyBindings">
        <structure>
            <property name="name">queryText</property>
            <property name="id">444</property>
            <expression name="value" type="javascript">var databaseIdentifier = Packages.com.enactor.core.utilities.PropertyRegistry.getCoreConfigs().getProperty(Packages.com.enactor.core.config.ICorePropertyConfigs.QUERY_TRANSLATOR_DATABASE_IDENTIFIER);
             	var dataFormatter = Packages.com.enactor.coreUI.factories.DataFormatterFactory.getInstance().getFormatter(reportContext.getLocale());
             	var sql = "SELECT (CASE WHEN COT.Description is null THEN COH.Customer_Order_Type_Id ELSE COT.Description END) REPORT_TYPE, COH.Status STATUS, Count(*) COUNTS, sum(COH.Order_Value) TOTAL_AMOUNT";
             	var frm = " FROM CustomerOrderHeaders COH left outer join CustomerOrderTypes COT on COH.Customer_Order_Type_Id = COT.Type_ID";	                 
                var whr = " WHERE 1=1";        
             	        if  (params["locationId"].value != "") {
 							whr = whr + " and COH.Created_At_Location_Id = '" + params["locationId"].value + "'";
 						} if (params["userId"].value != "") {
 							whr = whr + " and COH.Created_By_User_Id = '" + params["userId"].value + "'"; 							
 						} 
 				var whrDate = "";
 					   if (params["startDate"].value != "") { 							
 							whrDate = whrDate + " and COH.Created_Date >= '" + params["startDate"].value + "'"; 							
 						} if (params["endDate"].value != "") {
 							whrDate = whrDate + " and COH.Created_Date &lt;= '" + params["endDate"].value + "'"; 							
 						}
 				var grpBy = " Group by COH.Status, COT.Description, COH.Customer_Order_Type_Id";
 						 	
             	if (databaseIdentifier.equals(Packages.com.enactor.core.database.queryTranslators.QueryTranslatorFactory.SQL_SERVER_DATABASE)) {
             		frm = " FROM CustomerOrderHeaders COH WITH(NOLOCK) left outer join CustomerOrderTypes COT WITH(NOLOCK) on COH.Customer_Order_Type_Id = COT.Type_ID";           			
             		sql = sql + frm + whr + whrDate + grpBy;
             	} else if (databaseIdentifier.equals(Packages.com.enactor.core.database.queryTranslators.QueryTranslatorFactory.DB2_DATABASE || Packages.com.enactor.core.database.queryTranslators.QueryTranslatorFactory.DERBY_DATABASE)) {
             	 	if (params["startDate"].value != "") { 							
 						whrDate = " and COI.Created_Date >= '" + params["startDate"].value + "-00.00.00')"; 							
 					} if (params["endDate"].value != "") {
 					  	whrDate = whrDate + " and COI.Created_Date &lt;= '" + params["endDate"].value + "-23.59.59')"; 							
 					} 					
             	 	sql = sql + frm + whr + whrDate + grpBy + " WITH UR";
             	} if (databaseIdentifier.equals(Packages.com.enactor.core.database.queryTranslators.QueryTranslatorFactory.ORACLE_DATABASE)) {
             	 	if (params["startDate"].value != "") { 							
 						whrDate = " and COH.Created_Date >= to_date('" + params["startDate"].value + "', 'YYYY-MM-DD')"; 							
 					} if (params["endDate"].value != "") {
 					  	whrDate = whrDate + " and COH.Created_Date &lt;= to_date('" + params["endDate"].value + "', 'YYYY-MM-DD')"; 							
 					}    
 					sql = sql + frm + whr + whrDate + grpBy        	
             	} if (databaseIdentifier.equals(Packages.com.enactor.core.database.queryTranslators.QueryTranslatorFactory.MYSQL_DATABASE)) {
             	 	  
 					sql = "SET SESSION TRANSACTION ISOLATION LEVEL READ UNCOMMITTED; " + sql + frm + whr + whrDate + grpBy        	
             	} else {
             	    sql = sql + frm + whr + whrDate + grpBy;            	 
             	}   
      	  	     	            	           	        
           sql;
           </expression>
        </structure>
