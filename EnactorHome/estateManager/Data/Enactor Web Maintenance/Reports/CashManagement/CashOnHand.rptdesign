                                                <property name="contentType">html</property>
                                                <text-property name="content"><![CDATA[<div id="textcontrol" title="<value-of>data["TENDER_DESCRIPTION"];</value-of>"><value-of>
currencyVal = data["TENDER_VALUE/terminal/tender"] != null ? data["TENDER_VALUE/terminal/tender"] : 0;
