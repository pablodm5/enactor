            <property name="dataSource">Data Source</property>
            <xml-property name="queryText"><![CDATA[select RTI.Location_Id as LOCATION_ID,
RTI.Base_Currency_Id as BASE_CURRENCY_ID,
CMT.Session_Owner_Id as TERMINAL_ID,
CSO.Name as TERMINAL_NAME,
CMT.Tender_Id as TENDER_ID,
T.Description as TENDER_DESCRIPTION,
sum(CMT.Tender_Base_Value) as TENDER_BASE_VALUE,
CMT.Currency_Id as CURRENCY_ID,
sum(CMT.Tender_Value) as TENDER_VALUE
from CashManagementTenders CMT
inner join RetailTransactionIds RTI on CMT.Transaction_Key = RTI.Transaction_Key
inner join
(
	select max(End_Date) as End_Date,Location_Id,Owner_Id
        from CashSessions 
        where (? = !~''~! or Location_Id = ?) 
        and (End_Date is null or End_Date < ?)
        and (Type != 4)
        group by Location_Id,Owner_Id
) KEYVT on (KEYVT.End_Date is null or KEYVT.End_Date <= RTI.Completed_Date_Time) and KEYVT.Location_Id = RTI.Location_Id and KEYVT.Owner_Id = CMT.Session_Owner_Id
left outer join CashSessionOwners CSO on CSO.Owner_Id = CMT.Session_Owner_Id and CSO.Type = CMT.Session_Owner_Type and CSO.Location_Id = RTI.Location_Id
left outer join (SELECT Tender_Id, MAX(Description) AS Description FROM Tenders GROUP BY Tender_Id) T ON T.Tender_Id = CMT.Tender_Id
where	(CMT.Session_Owner_Type != 4)
and not (CMT.Transaction_Type in (?, ?))
and 	(? = !~''~! or RTI.Location_Id = ?) 
and 	(RTI.Completed_Date_Time < ?)
group by RTI.Location_Id, CMT.Session_Owner_Id, CSO.Name, CMT.Tender_Id, T.Description, RTI.Base_Currency_Id, CMT.Currency_Id  
order by RTI.Location_Id, CMT.Session_Owner_Id]]></xml-property>
