            <structure name="cachedMetaData"/>
            <property name="dataSource">Data Source</property>
            <xml-property name="queryText"><![CDATA[select 
		RTI.Location_Id as LOCATION_ID,	
		RTI.Base_Currency_Id as BASE_CURRENCY_ID,
		PT.Promotion_Id as PROMOTION_ID,
		PT.Promotion_Description as PROMOTION_DESCRIPTION,
		PT.Reward_Type as REWARD_TYPE,
		SUM( PT.Quantity ) as QUANTITY,
		SUM( PT.Distributed_Reward_Value ) as DISTRIBUTED_REWARD_VALUE,
		SUM( 1 ) as HIT_COUNT
from 	PromotionTriggers PT
inner join RetailTransactionIds RTI on PT.Transaction_Key = RTI.Transaction_Key
where 	(? = !~''~! or RTI.Region_Group_Id = ?) 
and 	(? = !~''~! or RTI.Region_Group_Hierarchy_Id = ?) 
and 	(? = !~''~! or RTI.Location_Id = ?) 
and 	(? = !~''~! or RTI.Device_Id = ?) 
and 	(? = !~''~! or PT.Promotion_Id = ?)
and 	(? = !~''~! or PT.Promotion_Group_Id = ?)
and 	(? = !~''~! or PT.Promotion_Group_Hierarchy_Id = ?)
and 	(RTI.Completed_Date_Time >= ?)
and 	(RTI.Completed_Date_Time <= ?)
group by RTI.Location_Id, RTI.Base_Currency_Id, PT.Promotion_Id, PT.Promotion_Description, PT.Reward_Type
order by RTI.Location_Id, PT.Promotion_Id, PT.Promotion_Description, PT.Reward_Type]]></xml-property>
