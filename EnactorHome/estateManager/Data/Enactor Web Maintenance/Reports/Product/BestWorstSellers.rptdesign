            <xml-property name="queryText"><![CDATA[select 	RTI.Base_Currency_Id as BASE_CURRENCY_ID,
		PS.Product_Id as PRODUCT_ID,
		max( PS.Product_Description ) as PRODUCT_DESCRIPTION,
		PS.MMGroup_Id as MMGROUP_ID,
		PS.MMGroup_Hierarchy_Id as MMGROUP_HIERARCHY_ID,
		G.Name as MMGROUP_DESCRIPTION,
		sum( PS.Sales_Value ) as SALES_VALUE,
		sum( PS.Sales_Quantity ) as SALES_QUANTITY,
		SKUP.Style_Id as STYLE_ID,
		S.Description as SIZE_DESCRIPTION,
		C.Description as COLOUR_DESCRIPTION
from 	ProductSales PS
inner join RetailTransactionIds RTI on PS.Transaction_Key = RTI.Transaction_Key
left outer join Groups G on G.Group_Type_Id = ? and G.Group_Id = PS.MMGroup_Id and G.Group_Hierarchy_Id = PS.MMGroup_Hierarchy_Id
and G.Variant_Group_Type_Id = ? and G.Variant_Group_Hierarchy_Id = RTI.Region_Group_Hierarchy_Id
left outer join Products P on PS.Product_Id = P.Product_Id
left outer join SKUProducts SKUP on PS.Product_Id = SKUP.Product_Id
left outer join Sizes S on SKUP.Size_Id = S.Size_Id and SKUP.Size_Range_Id = S.Size_Range_Id
left outer join Colours C on SKUP.Colour_Id = C.Colour_Id and SKUP.Colour_Range_Id = C.Colour_Range_Id 
where (? = '' or RTI.Region_Group_Id = ?) 
and (? = '' or RTI.Region_Group_Hierarchy_Id = ?) 
and (? = '' or RTI.Location_Id = ?) 
and (? = '' or RTI.Device_Id = ?) 
and (? = '' or (G.Level_1_Group_Id = ? or 
			G.Level_2_Group_Id = ? or
			G.Level_3_Group_Id = ? or
			G.Level_4_Group_Id = ? or
			G.Level_5_Group_Id = ? or
			G.Level_6_Group_Id = ? or
			G.Level_7_Group_Id = ? or
			G.Level_8_Group_Id = ? or
			G.Level_9_Group_Id = ? or
			G.Level_10_Group_Id = ?))
and (? = '' or PS.MMGroup_Hierarchy_Id = ?)
and (RTI.Completed_Date_Time >= ?)
and (RTI.Completed_Date_Time <= ?)
group by RTI.Base_Currency_Id, PS.Product_Id, PS.MMGroup_Id, PS.MMGroup_Hierarchy_Id, G.Name, SKUP.STYLE_ID, S.Description, C.Description]]></xml-property>
