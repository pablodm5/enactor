            <xml-property name="queryText"><![CDATA[select 	PS.Product_Id as PRODUCT_ID,
		max( PS.Product_Description ) as PRODUCT_DESCRIPTION,
		RTI.Base_Currency_Id as BASE_CURRENCY_ID,
		sum( PS.Sales_Quantity ) as QUANTITY,
		sum( PS.Sales_Value ) as VALUE,
		max( IIL.Value ) - sum( PS.Sales_Quantity ) as CURRENT_STOCK_LEVEL,
		SKUP.Style_Id as STYLE,
		S.Description as SIZE_DESCRIPTION,
		C.Description as COLOUR_DESCRIPTION,
		PS.MMGroup_Id,
		G.Name
from 	ProductSales PS left outer join  (
			select	Inventory_Item_Id, Value
        	from    ItemInventoryLevels
            where   (Inventory_Type = ?)
            and 	(? = !~''~! or Location_Id = ?)) IIL ON PS.Product_Id = IIL.Inventory_Item_Id
left outer join Products P on PS.Product_Id = P.Product_Id 
left outer join SKUProducts SKUP on PS.Product_Id = SKUP.Product_Id 
left outer join Sizes S on SKUP.Size_Id = S.Size_Id and SKUP.Size_Range_Id = S.Size_Range_Id
left outer join Colours C on SKUP.Colour_Id = C.Colour_Id and SKUP.Colour_range_Id = C.Colour_Range_Id
inner join RetailTransactionIds RTI on PS.Transaction_Key = RTI.Transaction_Key
left outer join Groups G on PS.MMGroup_Id = G.Group_Id and PS.MMGroup_Hierarchy_Id = G.Group_Hierarchy_Id and G.Group_Type_Id = ?
where 	(? = !~''~! or PS.MMGroup_Id IN (
	select 	G.Group_Id
	from 	Groups G 
	where 	(G.Group_Type_Id = ?) 
	and 	(? = !~''~! or G.Group_Hierarchy_Id = ?) 
	and 	(G.Level_1_Group_Id = ? or 
			G.Level_2_Group_Id = ? or
			G.Level_3_Group_Id = ? or
			G.Level_4_Group_Id = ? or
			G.Level_5_Group_Id = ? or
			G.Level_6_Group_Id = ? or
			G.Level_7_Group_Id = ? or
			G.Level_8_Group_Id = ? or
			G.Level_9_Group_Id = ? or
			G.Level_10_Group_Id = ?)))
and 	(? = !~''~! or PS.MMGroup_Hierarchy_Id = ?)
and 	(RTI.Completed_Date_Time > ?)
group by PS.Product_Id, RTI.Base_Currency_Id, SKUP.Style_Id, S.Description, C.Description, PS.MMGroup_Id, G.Name
order by PS.MMGroup_Id, PS.Product_Id, MAX(PS.Product_Description)]]></xml-property>
