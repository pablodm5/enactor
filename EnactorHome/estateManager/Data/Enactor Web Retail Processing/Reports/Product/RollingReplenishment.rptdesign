    <method name="initialize"><![CDATA[globalDataFormatter = Packages.com.enactor.coreUI.factories.DataFormatterFactory.getInstance().getFormatter(reportContext.getLocale());
baseCurrencyId = globalDataFormatter.getBaseCurrencyId();

if(params["locationId"].value == null){
	params["locationId"].value = "";
}

if(params["mmGroupId"].value == null){
	params["mmGroupId"].value = "";
}

if(params["mmGroupHierarchyId"].value == null){
	params["mmGroupHierarchyId"].value = ""; 
}

if(params["mmVariantGroupId"].value == null){
	params["mmVariantGroupId"].value = "";
}

if(params["mmVariantGroupHierarchyId"].value == null){
	params["mmVariantGroupHierarchyId"].value = ""; 
}

lastRunStr = "";

if(params["lastRun"].value != null){
	lastRunStr = globalDataFormatter.formatDate( params["lastRun"].value) + " " + globalDataFormatter.formatTime( params["lastRun"].value);
}
else {
	params["lastRun"].value = Packages.com.enactor.core.utilities.DateUtils.beginningOfTime();
	lastRunStr = reportContext.getMessage("BLANK", reportContext.getLocale());
}]]></method>
