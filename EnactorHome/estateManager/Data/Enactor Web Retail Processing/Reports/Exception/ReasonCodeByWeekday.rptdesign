            <property name="queryText">select 	RTI.Base_Currency_Id as BASE_CURRENCY_ID,
		R.Reason_Type as REASON_TYPE,
		TE.Reason_Code as REASON_ID,
		R.Description as REASON_DESCRIPTION,
		sum( case when P.Day_Of_Week = 1 then -TE.Exception_Value else 0 end ) as BASE_VALUE_1,
		sum( case when P.Day_Of_Week = 2 then -TE.Exception_Value else 0 end ) as BASE_VALUE_2,
		sum( case when P.Day_Of_Week = 3 then -TE.Exception_Value else 0 end ) as BASE_VALUE_3,
		sum( case when P.Day_Of_Week = 4 then -TE.Exception_Value else 0 end ) as BASE_VALUE_4,
		sum( case when P.Day_Of_Week = 5 then -TE.Exception_Value else 0 end ) as BASE_VALUE_5,
		sum( case when P.Day_Of_Week = 6 then -TE.Exception_Value else 0 end ) as BASE_VALUE_6,
		sum( case when P.Day_Of_Week = 7 then -TE.Exception_Value else 0 end ) as BASE_VALUE_7,
		sum(-TE.Exception_Value) as BASE_VALUE
from 	TerminalExceptions TE
inner join RetailTransactionIds RTI on TE.Transaction_Key = RTI.Transaction_Key
inner join Periods P on RTI.Completed_Day = P.Day and RTI.Completed_Year = !~P.Year~!
left outer join Reasons R on TE.Reason_Code = R.Reason_Id
where 	(TE.Reason_Code &lt;> '-')
and 	(TE.Type in (?,?,?,?,?,?,?,?,?,?))
and 	(? = !~''~! or RTI.Region_Group_Id = ?) 
and 	(? = !~''~! or RTI.Region_Group_Hierarchy_Id = ?)
and 	(? = !~''~! or RTI.Location_Id = ?) 
and 	(? = !~''~! or RTI.Device_Id = ?) 
and 	(? = !~''~! or TE.User_Id = ?) 
and 	(? = !~''~! or R.Reason_Type = ?)
and 	(? = !~''~! or TE.Reason_Code = ?)
and 	(RTI.Completed_Date_Time >= ?)
and 	(RTI.Completed_Date_Time &lt;= ?)
group by RTI.Base_Currency_Id, R.Reason_Type, TE.Reason_Code, R.Description
union
select 	RTI.Base_Currency_Id as BASE_CURRENCY_ID,
		R.Reason_Type as REASON_TYPE,
		CMT.Reason_Id as REASON_ID,
		R.Description as REASON_DESCRIPTION,
		sum( case when P.Day_Of_Week = 1 then ( case when R.Reason_Type in (?,?,?,?) then CMT.Tender_Base_Value else -CMT.Tender_Base_Value end ) else 0 end ) as BASE_VALUE_1,
		sum( case when P.Day_Of_Week = 2 then ( case when R.Reason_Type in (?,?,?,?) then CMT.Tender_Base_Value else -CMT.Tender_Base_Value end ) else 0 end ) as BASE_VALUE_2,
		sum( case when P.Day_Of_Week = 3 then ( case when R.Reason_Type in (?,?,?,?) then CMT.Tender_Base_Value else -CMT.Tender_Base_Value end ) else 0 end ) as BASE_VALUE_3,
		sum( case when P.Day_Of_Week = 4 then ( case when R.Reason_Type in (?,?,?,?) then CMT.Tender_Base_Value else -CMT.Tender_Base_Value end ) else 0 end ) as BASE_VALUE_4,
		sum( case when P.Day_Of_Week = 5 then ( case when R.Reason_Type in (?,?,?,?) then CMT.Tender_Base_Value else -CMT.Tender_Base_Value end ) else 0 end ) as BASE_VALUE_5,
		sum( case when P.Day_Of_Week = 6 then ( case when R.Reason_Type in (?,?,?,?) then CMT.Tender_Base_Value else -CMT.Tender_Base_Value end ) else 0 end ) as BASE_VALUE_6,
		sum( case when P.Day_Of_Week = 7 then ( case when R.Reason_Type in (?,?,?,?) then CMT.Tender_Base_Value else -CMT.Tender_Base_Value end ) else 0 end ) as BASE_VALUE_7,
		sum( case when R.Reason_Type in (?,?,?,?) then CMT.Tender_Base_Value else -CMT.Tender_Base_Value end ) as BASE_VALUE
from 	CashManagementTenders CMT
inner join RetailTransactionIds RTI on CMT.Transaction_Key = RTI.Transaction_Key
inner join Periods P on RTI.Completed_Day = P.Day and RTI.Completed_Year = !~P.Year~!
left outer join Reasons R on CMT.Reason_Id = R.Reason_Id
where 	(CMT.Reason_Id &lt;> '-')
and 	(CMT.Transaction_Type in (?,?,?,?,?))
and 	(? = !~''~! or RTI.Region_Group_Id = ?) 
and 	(? = !~''~! or RTI.Region_Group_Hierarchy_Id = ?)
and 	(? = !~''~! or RTI.Location_Id = ?) 
and 	(? = !~''~! or RTI.Device_Id = ?) 
and 	(? = !~''~! or CMT.User_Id = ?) 
and 	(? = !~''~! or R.Reason_Type = ?)
and 	(? = !~''~! or CMT.Reason_Id = ?)
and 	(RTI.Completed_Date_Time >= ?)
and 	(RTI.Completed_Date_Time &lt;= ?)
group by RTI.Base_Currency_Id, R.Reason_Type, CMT.Reason_Id, R.Description
order by BASE_CURRENCY_ID, REASON_TYPE, REASON_ID, REASON_DESCRIPTION
</property>
