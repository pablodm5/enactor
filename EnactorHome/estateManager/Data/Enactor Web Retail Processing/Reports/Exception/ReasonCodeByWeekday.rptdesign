    <method name="initialize"><![CDATA[globalDataFormatter = Packages.com.enactor.coreUI.factories.DataFormatterFactory.getInstance().getFormatter(reportContext.getLocale());
baseCurrencyId = globalDataFormatter.getBaseCurrencyId();

if(params["regionGroupId"].value == null){
	params["regionGroupId"].value = "";
}

if(params["regionGroupHierarchyId"].value == null){
	params["regionGroupHierarchyId"].value = "";
}

if(params["locationId"].value == null){
	params["locationId"].value = "";
}

if(params["deviceId"].value == null){
	params["deviceId"].value = "";
}

if(params["userId"].value == null){
	params["userId"].value = "";
}

if(params["reasonType"].value == null){
	params["reasonType"].value = "";
}

if(params["reasonId"].value == null){
	params["reasonId"].value = "";
}

startDateStr = "";

if(params["startDate"].value != null){
	startDateStr = globalDataFormatter.formatDate(params["startDate"].value);
}
else {
	params["startDate"].value = Packages.com.enactor.core.utilities.DateUtils.beginningOfTime();
	startDateStr = reportContext.getMessage("BLANK", reportContext.getLocale());
}

endDateStr = "";

if(params["endDate"].value != null){
	endDateStr = globalDataFormatter.formatDate(params["endDate"].value);
}
else {
	params["endDate"].value = Packages.com.enactor.core.utilities.DateUtils.endOfTime();
	endDateStr = reportContext.getMessage("BLANK", reportContext.getLocale());
}

//uncomment one of the lines below as appropriate:
//real line:
//startDateDayOfWeek = Packages.com.enactor.mfc.period.actions.PeriodHelper.getStartOfWeekFromPeriods();

//preview line:
startDateDayOfWeek = 1;

//day descriptions
day1Desc = '';
day2Desc = '';
day3Desc = '';
day4Desc = '';
day5Desc = '';
day6Desc = '';
day7Desc = '';
switch( startDateDayOfWeek ) {
case 1: 
	day1Desc = reportContext.getMessage("SUNDAY", reportContext.getLocale()); 
	day2Desc = reportContext.getMessage("MONDAY", reportContext.getLocale()); 
	day3Desc = reportContext.getMessage("TUESDAY", reportContext.getLocale()); 
	day4Desc = reportContext.getMessage("WEDNESDAY", reportContext.getLocale()); 
	day5Desc = reportContext.getMessage("THURSDAY", reportContext.getLocale()); 
	day6Desc = reportContext.getMessage("FRIDAY", reportContext.getLocale()); 
	day7Desc = reportContext.getMessage("SATURDAY", reportContext.getLocale()); 
break;
case 2: 
	day1Desc = reportContext.getMessage("MONDAY", reportContext.getLocale()); 
	day2Desc = reportContext.getMessage("TUESDAY", reportContext.getLocale()); 
	day3Desc = reportContext.getMessage("WEDNESDAY", reportContext.getLocale()); 
	day4Desc = reportContext.getMessage("THURSDAY", reportContext.getLocale()); 
	day5Desc = reportContext.getMessage("FRIDAY", reportContext.getLocale()); 
	day6Desc = reportContext.getMessage("SATURDAY", reportContext.getLocale()); 
	day7Desc = reportContext.getMessage("SUNDAY", reportContext.getLocale()); 
break;
case 3: 
	day1Desc = reportContext.getMessage("TUESDAY", reportContext.getLocale()); 
	day2Desc = reportContext.getMessage("WEDNESDAY", reportContext.getLocale()); 
	day3Desc = reportContext.getMessage("THURSDAY", reportContext.getLocale()); 
	day4Desc = reportContext.getMessage("FRIDAY", reportContext.getLocale()); 
	day5Desc = reportContext.getMessage("SATURDAY", reportContext.getLocale());
	day6Desc = reportContext.getMessage("SUNDAY", reportContext.getLocale()); 
	day7Desc = reportContext.getMessage("MONDAY", reportContext.getLocale());  
break;
case 4:  
	day1Desc = reportContext.getMessage("WEDNESDAY", reportContext.getLocale()); 
	day2Desc = reportContext.getMessage("THURSDAY", reportContext.getLocale()); 
	day3Desc = reportContext.getMessage("FRIDAY", reportContext.getLocale()); 
	day4Desc = reportContext.getMessage("SATURDAY", reportContext.getLocale());
	day5Desc = reportContext.getMessage("SUNDAY", reportContext.getLocale()); 
	day6Desc = reportContext.getMessage("MONDAY", reportContext.getLocale());
	day7Desc = reportContext.getMessage("TUESDAY", reportContext.getLocale());  
break;
case 5: 
	day1Desc = reportContext.getMessage("THURSDAY", reportContext.getLocale()); 
	day2Desc = reportContext.getMessage("FRIDAY", reportContext.getLocale()); 
	day3Desc = reportContext.getMessage("SATURDAY", reportContext.getLocale());
	day4Desc = reportContext.getMessage("SUNDAY", reportContext.getLocale()); 
	day5Desc = reportContext.getMessage("MONDAY", reportContext.getLocale());
	day6Desc = reportContext.getMessage("TUESDAY", reportContext.getLocale());
	day7Desc = reportContext.getMessage("WEDNESDAY", reportContext.getLocale());    
break;
case 6: 
	day1Desc = reportContext.getMessage("FRIDAY", reportContext.getLocale()); 
	day2Desc = reportContext.getMessage("SATURDAY", reportContext.getLocale());
	day3Desc = reportContext.getMessage("SUNDAY", reportContext.getLocale()); 
	day4Desc = reportContext.getMessage("MONDAY", reportContext.getLocale());
	day5Desc = reportContext.getMessage("TUESDAY", reportContext.getLocale());
	day6Desc = reportContext.getMessage("WEDNESDAY", reportContext.getLocale()); 
	day7Desc = reportContext.getMessage("THURSDAY", reportContext.getLocale());   
break;
case 7: 
	day1Desc = reportContext.getMessage("SATURDAY", reportContext.getLocale());
	day2Desc = reportContext.getMessage("SUNDAY", reportContext.getLocale()); 
	day3Desc = reportContext.getMessage("MONDAY", reportContext.getLocale());
	day4Desc = reportContext.getMessage("TUESDAY", reportContext.getLocale());
	day5Desc = reportContext.getMessage("WEDNESDAY", reportContext.getLocale()); 
	day6Desc = reportContext.getMessage("THURSDAY", reportContext.getLocale()); 
	day7Desc = reportContext.getMessage("FRIDAY", reportContext.getLocale());   
break;
}
//]]></method>
