            <structure name="cachedMetaData"/>
            <property name="dataSource">Data Source</property>
            <list-property name="resultSet"/>
            <xml-property name="queryText"><![CDATA[select 	E.Employee_Id as EMPLOYEE_NUMBER,
		E.Forename as EMPLOYEE_FORENAME,
		E.Surname as EMPLOYEE_SURNAME,
		MT.Employee_Card_Number as CARD_NUMBER,
		E.Employee_Discount_Allowance as MONTHLY_ALLOWANCE,
		sum( case when 
			(RTI.Completed_Date_Time >= ? and RTI.Completed_Date_Time <= ?) then MT.Sales_Value + MT.Orders_Value 
			else 0 end ) as TOTAL_SPEND,
		AB.Account_Balance as REMAINING_BALANCE,
		AB.Balance_Date as LAST_SPEND_DATE,
		EA.Status as ACCOUNT_STATUS,
		AB.Currency_Id as BASE_CURRENCY_ID
from 	Employees E
inner join EmployeeAccounts EA on E.Employee_Id = EA.Account_Id
inner join AccountBalances AB on E.Employee_Id = AB.Account_Id and EA.Account_Type = AB.Account_Type
left outer join MerchandiseTransactions MT on E.Employee_Id = MT.Employee_Id
left outer join RetailTransactionIds RTI on (MT.Transaction_Key = RTI.Transaction_Key)
where 	(EA.Account_Type = ?)
and 	(? = !~''~! or RTI.Region_Group_Id = ?) 
and 	(? = !~''~! or RTI.Region_Group_Hierarchy_Id = ?) 
and 	(? = !~''~! or RTI.Location_Id = ?) 
group by E.Employee_Id, E.Employee_Discount_Allowance, MT.Employee_Card_Number, EA.Status, AB.Account_Balance, AB.Balance_Date, E.Forename, E.Surname, AB.Currency_Id
order by E.Employee_Id, E.Forename, E.Surname
]]></xml-property>
        </oda-data-set>
    </data-sets>
