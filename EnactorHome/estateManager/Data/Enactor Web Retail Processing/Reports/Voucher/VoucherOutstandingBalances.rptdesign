    <method name="initialize"><![CDATA[globalDataFormatter = Packages.com.enactor.coreUI.factories.DataFormatterFactory.getInstance().getFormatter(reportContext.getLocale());
baseCurrencyId = globalDataFormatter.getBaseCurrencyId();

if(params["voucherStatus"].value == null){
	params["voucherStatus"].value = "ISSUED";
}

if(params["voucherTypeId"].value == null){
	params["voucherTypeId"].value = "";
}

if(params["voucherTypeRegionId"].value == null){
	params["voucherTypeRegionId"].value = "";
}

if(params["voucherTypeRegionHierarchyId"].value == null){
	params["voucherTypeRegionHierarchyId"].value = "";
}

issueStartDateStr = "";
params['hasIssueStartDate'].value = 0;

if(params["issueStartDate"].value != null){
	issueStartDateStr = globalDataFormatter.formatDate(params["issueStartDate"].value);
	params['hasIssueStartDate'].value = 1;
}
else {
	params["issueStartDate"].value = Packages.com.enactor.core.utilities.DateUtils.beginningOfTime();
	issueStartDateStr = reportContext.getMessage("BLANK", reportContext.getLocale());
}

issueEndDateStr = "";
params['hasIssueEndDate'].value = 0;

if(params["issueEndDate"].value != null){
	issueEndDateStr = globalDataFormatter.formatDate(params["issueEndDate"].value);
	params['hasIssueEndDate'].value = 1;
}
else {
	params["issueEndDate"].value = Packages.com.enactor.core.utilities.DateUtils.endOfTime();
	issueEndDateStr = reportContext.getMessage("BLANK", reportContext.getLocale());
}

expiryStartDateStr = "";
params['hasExpiryStartDate'].value = 0;

if(params["expiryStartDate"].value != null){
	expiryStartDateStr = globalDataFormatter.formatDate(params["expiryStartDate"].value);
	params['hasExpiryStartDate'].value = 1;
}
else {
	params["expiryStartDate"].value = Packages.com.enactor.core.utilities.DateUtils.beginningOfTime();
	expiryStartDateStr = reportContext.getMessage("BLANK", reportContext.getLocale());
}

expiryEndDateStr = "";
params['hasExpiryEndDate'].value = 0;

if(params["expiryEndDate"].value != null){
	expiryEndDateStr = globalDataFormatter.formatDate(params["expiryEndDate"].value);
	params['hasExpiryEndDate'].value = 1;
}
else {
	params["expiryEndDate"].value = Packages.com.enactor.core.utilities.DateUtils.endOfTime();
	expiryEndDateStr = reportContext.getMessage("BLANK", reportContext.getLocale());
}]]></method>
