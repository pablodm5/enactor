            <xml-property name="queryText"><![CDATA[select 	VT.Description as DESCRIPTION,
		VSN.Voucher_Type_Id as VOUCHER_TYPE_ID,
		VSN.Serial_Number as SERIAL_NUMBER,
		VSN.Voucher_Value as VOUCHER_VALUE,
		VSN.Issue_Date as ISSUE_DATE,
		VSN.Expiry_Date as EXPIRY_DATE,
		VSN.Currency_Id as BASE_CURRENCY_ID
from VoucherSerialNumbers VSN
left outer join VoucherTypes VT on VSN.Voucher_Type_Id = VT.Voucher_Type_Id and VSN.Region_Id = VT.Region_Id and VSN.Region_Hierarchy_Id = VT.Region_Hierarchy_Id and VSN.Region_Type_Id = VT.Region_Type_Id 
where 	(VSN.Status = ?)
and 	(? = '' or VSN.Voucher_Type_Id = ?)
and 	(? = '' or VSN.Region_Id = ?)
and 	(? = '' or VSN.Region_Hierarchy_Id = ?)
and 	(? = 0 or VSN.Issue_Date >= ?)
and 	(? = 0 or VSN.Issue_date <= ?)
and 	(? = 0 or VSN.Expiry_Date >= ?)
and 	(? = 0 or VSN.Expiry_date <= ?)
order by VT.Description, VSN.Voucher_Type_Id, VSN.Issue_Date, VSN.Expiry_Date, VSN.Serial_Number
