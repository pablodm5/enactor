            <property name="dataSource">Data Source</property>
            <xml-property name="queryText"><![CDATA[
SELECT 
  CASE WHEN CTAll.Original_Transaction_ID = '-' THEN CTAll.Transaction_ID ELSE CTAll.Original_Transaction_ID END AS ACTUAL_ORIGINAL_TRANSACTION_ID,
  CTOriginal.Effective_Date as EVENT_DATE, 
  CTOriginal.Location_Id as LOCATION_ID, 
  CTOriginal.Base_Currency_Id as BASE_CURRENCY_ID, 
  L.Description as LOCATION_DESCRIPTION, 
  CTOriginal.Owner_Id as OWNER_ID, 
  CSO.Name as OWNER_NAME, 
  CTI.Tender_Id as TENDER_ID, 
  T.Description as TENDER_DESCRIPTION, 
  SUM(
    CASE WHEN CTI.Item_Type = ? THEN (
      CASE WHEN ? = 1 THEN - CTI.Value ELSE CTI.Value END
    ) ELSE 0 END
  ) AS COUNTED_VALUE, 
  SUM(
    CASE WHEN CTI.Item_Type = ? THEN (
      CASE WHEN ? = 1 THEN - CTI.Base_Value ELSE CTI.Base_Value END
    ) ELSE 0 END
  ) AS COUNTED_BASE_VALUE, 
  MAX(
    CASE WHEN CTI.Item_Type = ? THEN CTI.Value ELSE 0 END
  ) AS EXPECTED_VALUE, 
  MAX(
    CASE WHEN CTI.Item_Type = ? THEN CTI.Base_Value ELSE 0 END
  ) AS EXPECTED_BASE_VALUE, 
  CTI.Currency_Id as CURRENCY_ID 
from 
  CashTransactions CTAll 
  INNER JOIN CashTransactionItems CTI ON CTAll.Id = CTI.Id 
  INNER JOIN CashTransactions CTOriginal ON (
    (
      CASE WHEN CTAll.Original_Application_Id = '-' THEN CTAll.Application_Id ELSE CTAll.Original_Application_Id END
    ) = CTOriginal.Application_Id
  ) 
  and (
    (
      CASE WHEN CTAll.Original_Transaction_ID = '-' THEN CTAll.Transaction_ID ELSE CTAll.Original_Transaction_ID END
    ) = CTOriginal.Transaction_Id
  ) 
  LEFT OUTER JOIN CashSessionOwners CSO ON CSO.Owner_Id = CTOriginal.Owner_Id 
  AND CSO.Type = CTOriginal.Owner_Type 
  AND CSO.Location_Id = CTOriginal.Location_Id 
  LEFT OUTER JOIN (
    SELECT 
      Tender_Id, 
      MAX(Description) AS Description 
    FROM 
      Tenders 
    GROUP BY 
      Tender_Id
  ) T ON T.Tender_Id = CTI.Tender_Id 
  LEFT OUTER JOIN Locations L ON L.Location_Id = CTOriginal.Location_Id 
  AND L.Location_Type = ? 
WHERE 
  (CTOriginal.Is_Voided = 0) 
  AND (
    CTOriginal.Transaction_Type = ?
  ) 
  AND (
    ? = !~''~! 
    or CTOriginal.Location_Id = ?
  ) 
  AND (
    ? = !~''~!
    or CTOriginal.Owner_Id = ?
  ) 
  AND (
    ? = 0 
    or CTOriginal.Owner_Type = ?
  ) 
  AND (
    CTOriginal.Effective_Date >= ?
  ) 
  AND (
    CTOriginal.Effective_Date <= ?
  ) 
GROUP BY 
  CASE WHEN CTAll.Original_Transaction_ID = '-' THEN CTAll.Transaction_ID ELSE CTAll.Original_Transaction_ID END, 
  CTI.Tender_ID,
  CTOriginal.Effective_Date,
  CTOriginal.Location_Id,
  CTOriginal.Base_Currency_Id,
  L.Description, 
  CTOriginal.Owner_Id, 
  CSO.Name,
  T.Description,
  CTI.Currency_Id
HAVING 
  ABS((SUM(
    CASE WHEN CTI.Item_Type = ? THEN (
      CASE WHEN ? = 1 THEN - CTI.Base_Value ELSE CTI.Base_Value END
    ) ELSE 0 END
  )) - (MAX(
    CASE WHEN CTI.Item_Type = ? THEN CTI.Base_Value ELSE 0 END
  ))) > ?
ORDER BY CTOriginal.Location_Id, CTOriginal.Owner_Id, CTOriginal.Effective_Date, T.Description
]]></xml-property>
