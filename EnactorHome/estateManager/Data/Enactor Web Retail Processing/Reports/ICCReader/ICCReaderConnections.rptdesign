            <property name="queryText">Select
	VT.Location_Id as LOCATION_ID, 
	Case when loc.Description is null THEN VT.Location_Id ELSE loc.Description END as LOCATION_NAME,
	Case when dev.Device_Name is null THEN VT.Device_Id ELSE dev.Device_Name END as DEVICE_NAME, 
	dev.Host_Name as HOST_NAME,
	(VT.Information) as DISCONNECTED_SERIAL_NUMBER,
	VT.Event_Local_Date_Time as DISCONNECTED_DATE,
	VTSortId.Information as CONNECTED_SERIAL_NUMBER,
	VTSortId.Event_Local_Date_Time as CONNECTED_DATE,
	(VTSortId.Event_Date_Long - VT.Event_Date_Long) as TIME_DISCONNECTED
from
(
	Select
		VTDisc.Location_Id as Location_Id,
		VTDisc.Device_Id as Device_Id,
		VTDisc.Information as Information, 
		VTDisc.Event_Local_Date_Time as Event_Local_Date_Time,
		VTDisc.Event_Date_Long as Event_Date_Long,
		min(VTConn.Event_Date_Event_Id) as Event_Date_Event_Id
	From
	(
		Select
			a.Location_Id as Location_Id,
			a.Device_Id as Device_Id,
			a.Information as Information,
			a.Event_Local_Date_Time as Event_Local_Date_Time,
			a.Event_Date_Long as Event_Date_Long
		From SystemEventLog a
		Left Outer Join
			Locations loc1 on a.Location_Id = loc1.Location_Id
		Left Outer Join
			Groups rgn1 on rgn1.Group_Type_Id = ? AND loc1.Region_id = rgn1.Group_id AND loc1.Region_Hierarchy_Id = rgn1.Group_Hierarchy_id
		Left Outer Join
			Devices dev1 on a.Device_Id = dev1.Device_Id AND a.Location_Id = dev1.Location_Id
		Where
			(	a.Event_Type_Id = ?
			OR	a.Event_Type_Id = ?)
		AND (? = !~''~! OR rgn1.Group_Id = ?)
		AND (? = !~''~! OR rgn1.Group_Hierarchy_Id = ?)
		AND (? = !~''~! OR loc1.Location_Id = ?)
		AND (a.Event_Local_Date_Time >= ?)
		AND (a.Event_Local_Date_Time &lt;= ?)
	) VTDisc
	Left Outer Join
	(
		Select
			b.Location_Id as Location_Id,
			b.Device_Id as Device_Id, 
			b.Event_Date_Long as Event_Date_Long,
			b.Event_Date_Event_Id as Event_Date_Event_Id
		From SystemEventLog b
		Left Outer Join
			Locations loc2 on b.Location_Id = loc2.Location_Id
		Left Outer Join
			Groups rgn2 on rgn2.Group_Type_Id = ? AND loc2.Region_id = rgn2.Group_id AND loc2.Region_Hierarchy_Id = rgn2.Group_Hierarchy_id
		Left Outer Join
			Devices dev2 on b.Device_Id = dev2.Device_Id AND b.Location_Id = dev2.Location_Id
		Where
			(	b.Event_Type_Id = ?
			OR	b.Event_Type_Id = ?)
		AND (? = !~''~! OR rgn2.Group_Id = ?)
		AND (? = !~''~! OR rgn2.Group_Hierarchy_Id = ?)
		AND (? = !~''~! OR loc2.Location_Id = ?)
		AND (b.Event_Local_Date_Time >= ?)
		AND (b.Event_Local_Date_Time &lt;= ?)
	) VTConn on VTDisc.Location_Id = VTConn.Location_ID
		AND VTDisc.Device_Id = VTConn.Device_ID
		AND VTConn.Event_Date_Long > VTDisc.Event_Date_Long
	Group by VTDisc.Location_Id, VTDisc.Device_Id, VTDisc.Information, VTDisc.Event_Local_Date_Time, VTDisc.Event_Date_Long
) VT
Left Outer Join
(
	Select
		c.Event_Date_Event_Id as Event_Date_Event_Id,
		c.Information as Information,
		c.Event_Local_Date_Time as Event_Local_Date_Time,
		c.Event_Date_Long as Event_Date_Long
	From SystemEventLog c
	Left Outer Join
		Locations loc3 on c.Location_Id = loc3.Location_Id
	Left Outer Join
		Groups rgn3 on rgn3.Group_Type_Id = ? AND loc3.Region_id = rgn3.Group_id AND loc3.Region_Hierarchy_Id = rgn3.Group_Hierarchy_id
	Where
		(	c.Event_Type_Id = ?
		OR	c.Event_Type_Id = ?)
		AND (? = !~''~! OR rgn3.Group_Id = ?)
		AND (? = !~''~! OR rgn3.Group_Hierarchy_Id = ?)
		AND (? = !~''~! OR loc3.Location_Id = ?)
		AND (c.Event_Local_Date_Time >= ?)
		AND (c.Event_Local_Date_Time &lt;= ?)
) VTSortId on VT.Event_Date_Event_Id = VTSortId.Event_Date_Event_Id
Left Outer Join
	Locations loc on VT.Location_Id = loc.Location_Id
Left Outer Join
	Devices dev on VT.Device_Id = dev.Device_Id AND VT.Location_Id = dev.Location_Id</property>
