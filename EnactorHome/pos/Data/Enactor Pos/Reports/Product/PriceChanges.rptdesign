            <xml-property name="queryText"><![CDATA[select P.MMGROUP_ID as MMGROUP_ID,
		G.NAME as MMGROUP_DESCRIPTION,
		LPP.PRODUCT_ID as PRODUCT_ID,
		P.DESCRIPTION as DESCRIPTION,
		LPP.PRICE as PRICE,
		LPP.START_DATE as START_DATE,
		CASE WHEN LPP.START_DATE is null or LPP.START_DATE < ? THEN 1 ELSE 0 END AS IS_START_DATE_B4_START_PARAM, 
		CASE WHEN LPP.START_DATE is null then ? else LPP.START_DATE end as NON_NULL_START_DATE,
		CASE WHEN LPP.END_DATE is null then ? else LPP.END_DATE end as NON_NULL_END_DATE
from	Locations L 
inner join ProductPrices LPP on L.LOCATION_ID = LPP.LOCATION_ID or (
	L.PRICE_GROUP_ID = LPP.GROUP_ID and L.PRICE_GROUP_HIERARCHY_ID = LPP.GROUP_HIERARCHY_ID and ? = LPP.GROUP_TYPE_ID
	and L.PRICE_VAR_GROUP_ID = LPP.VARIANT_GROUP_ID and L.PRICE_VAR_GROUP_HIERARCHY_ID = LPP.VARIANT_GROUP_HIERARCHY_ID and ? = LPP.VARIANT_GROUP_TYPE_ID)
left outer join Products P on P.PRODUCT_ID = LPP.PRODUCT_ID
left outer join Groups G on G.GROUP_ID = P.MMGROUP_ID and G.GROUP_HIERARCHY_ID = P.MMGROUP_HIERARCHY_ID and G.GROUP_TYPE_ID = ? 
inner join
(
	select	PIn.MMGROUP_ID as MMGROUP_ID_IN,
			PIn.MMGROUP_HIERARCHY_ID as MMGROUP_HIERARCHY_ID_IN,
			count(distinct LPPIn.PRODUCT_ID) as PRODUCT_ID_DISTINCT_COUNT,
			COUNT(LPPIn.PRODUCT_ID) as PRODUCT_ID_ALL_COUNT
	from 	ProductPrices LPPIn
	left outer join Products PIn on PIn.PRODUCT_ID = LPPIn.PRODUCT_ID
	where 	(LPPIn.LOCATION_ID = '-' or LPPIn.LOCATION_ID is null or ? = !~''~! or LPPIn.LOCATION_ID = ?)  
	and		(LPPIn.START_DATE >= ?)
	and		(LPPIn.START_DATE <= ?)
	group by PIn.MMGROUP_ID, PIn.MMGROUP_HIERARCHY_ID
) VT on VT.MMGROUP_ID_IN = P.MMGROUP_ID and VT.MMGROUP_HIERARCHY_ID_IN = P.MMGROUP_HIERARCHY_ID
where 	(? = !~''~! or L.LOCATION_ID = ?) 
and		(LPP.START_DATE is null or LPP.START_DATE <= ?)
and		(LPP.END_DATE is null or LPP.END_DATE >= ?)
order by PRODUCT_ID,
IS_START_DATE_B4_START_PARAM desc, 
NON_NULL_START_DATE desc, 
NON_NULL_END_DATE asc
]]></xml-property>
