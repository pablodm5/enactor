            <property name="dataSource">Data Source</property>
            <xml-property name="queryText"><![CDATA[select 	VTSales.USER_ID as USER_ID,
		U.Forename as USER_FORENAME,
		U.Surname as USER_SURNAME,
		COUNT( DISTINCT VTQty.TRANSACTION_KEY ) as TRANS_COUNT,
		sum( VTSales.SALES_VALUE ) as SALES_VALUE,
		sum( VTSales.SALES_QUANTITY ) as SALES_QUANTITY,
		sum( VTSales.ORDERS_VALUE ) as ORDERS_VALUE,
		sum( VTSales.ORDERS_QUANTITY ) as ORDERS_QUANTITY,
		sum( -VTSales.RETURNS_VALUE ) as RETURNS_VALUE,
		sum( -VTSales.RETURNS_QUANTITY ) as RETURNS_QUANTITY,
		sum( -VTSales.PRICE_OVERRIDES_VALUE ) as PRICE_OVERRIDES_VALUE,
		sum( -VTSales.DISCOUNTS_VALUE ) as DISCOUNTS_VALUE,
		sum( -VTSales.PROMOTIONS_VALUE ) as PROMOTIONS_VALUE,
		sum( VTQty.ONE_ITEM ) as ONE_ITEM,
		sum( VTQty.TWO_ITEM ) as TWO_ITEM,
		sum( VTQty.THREE_ITEM ) as THREE_ITEM,
		sum( VTQty.OTHER_ITEM ) as OTHER_ITEM
from (
	select	case when ? = 'Salesperson' then PS.Salesperson_Id else PS.User_Id end as USER_ID,
			RTI.Transaction_Key as TRANSACTION_KEY,
			sum( PS.Sales_Value ) as SALES_VALUE,
			sum( PS.Sales_Quantity ) as SALES_QUANTITY,
			sum( PS.Orders_Value ) as ORDERS_VALUE,
			sum( PS.Orders_Quantity ) as ORDERS_QUANTITY,
			sum( PS.Returns_Value ) as RETURNS_VALUE,
			sum( PS.Returns_Quantity ) as RETURNS_QUANTITY,
			sum( PS.Price_Overrides_Value ) as PRICE_OVERRIDES_VALUE,
			sum( PS.Discounts_Value ) as DISCOUNTS_VALUE,
			sum( PS.Promotions_Value ) as PROMOTIONS_VALUE
	from 	RetailTransactionIds RTI
	inner join MerchandiseTransactions MT on MT.Transaction_Key = RTI.Transaction_Key
	inner join ProductSales PS on RTI.Transaction_Key = PS.Transaction_Key
	where 	(? = !~''~! or RTI.Location_Id = ?) 
	and 	(? = !~''~! or RTI.Device_Id = ?) 
	and 	(? = !~''~! or (? = 'Salesperson' and PS.Salesperson_Id = ?) or ((? = !~''~! or ? = 'User') and PS.User_Id = ?))
	and 	(? = !~''~! or MT.Customer_Id = ?)
	and 	RTI.Completed_Date_Time >= ?
	and 	RTI.Completed_Date_Time <= ?
	group by PS.User_Id, PS.Salesperson_Id, RTI.Transaction_key
) VTSales
inner join
(
	select 	MT.User_Id as USER_ID,
			MT.Transaction_Key as TRANSACTION_KEY,
			case when MT.Sales_Quantity + MT.Orders_Quantity = 1 then 1 else 0 end as ONE_ITEM,
			case when MT.Sales_Quantity + MT.Orders_Quantity = 2 then 1 else 0 end as TWO_ITEM,
			case when MT.Sales_Quantity + MT.Orders_Quantity = 3 then 1 else 0 end as THREE_ITEM,
			case when MT.Sales_Quantity + MT.Orders_Quantity > 3 then 1 else 0 end as OTHER_ITEM
	from 	RetailTransactionIds RTI
	inner join MerchandiseTransactions MT on MT.Transaction_Key = RTI.Transaction_Key
