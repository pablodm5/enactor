<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<retail:posPromptPage xmlns:core="http://www.enactor.com/core" xmlns:retail="http://www.enactor.com/retail" xmlns:ns4="http://www.enactor.com/crm" xmlns:hta="http://docs.oasis-open.org/ns/bpel4people/ws-humantask/api/200803" xmlns:ns6="http://www.apis-it.hr/fin/2012/types/f73" xmlns:ns7="http://www.w3.org/2000/09/xmldsig#" xmlns:ns8="http://www.enactor.com/retail/storedRetailTransaction/service" xmlns:ns9="http://www.fu.gov.si/" xmlns:ns10="http://www.enactor.com/addressLookup/service" xmlns:htd="http://docs.oasis-open.org/ns/bpel4people/ws-humantask/200803" xmlns:htt="http://docs.oasis-open.org/ns/bpel4people/ws-humantask/types/200803" xmlns:sref="http://docs.oasis-open.org/wsbpel/2.0/serviceref" xmlns:ns14="http://www.enactor.com/crm/customerLoyalty/service" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" hasBorders="FALSE" windowtype="view-panel" height="800" width="480" managementBeanClassname="com.enactor.pos.menu.LoadMenuBean" managementBeanName="loadMenu" messageBase="Pos/Discount/DiscountMessages" url="NextGen480x800/Pos/Discount/EnterDiscountAmount">
    <core:elements>
        <retail:menuPageElement menuExpression="#{loadMenu.footerMenu}" id="FooterMenu">
            <retail:menu menuCatagory="RETAIL" menuType="TREE">
                <retail:menuSetId></retail:menuSetId>
                <retail:roleId>
                    <retail:roleId></retail:roleId>
                    <retail:regionId groupTypeId="region"/>
                </retail:roleId>
                <retail:menuGroupId groupTypeId="menuGroup"/>
                <retail:menuDefaultGroupId groupTypeId="menuGroup"/>
                <retail:localeId/>
            </retail:menu>
            <retail:renderElement>FOOTER_BUTTONS_PAGE_REFERENCE.FooterRefPanel.Button1</retail:renderElement>
            <retail:renderElement>FOOTER_BUTTONS_PAGE_REFERENCE.FooterRefPanel.Button2</retail:renderElement>
            <retail:renderElement>FOOTER_BUTTONS_PAGE_REFERENCE.FooterRefPanel.Button3</retail:renderElement>
            <retail:renderElement>FOOTER_BUTTONS_PAGE_REFERENCE.FooterRefPanel.Button4</retail:renderElement>
        </retail:menuPageElement>
        <core:hidden id="DialogPanel"/>
        <core:hidden id="ReceiptPanel"/>
        <core:hidden id="SelectionList"/>
        <core:button adjustImagesToSize="false" visible="true" id="BackButton">
            <core:text>CancelPressed</core:text>
        </core:button>
        <core:label messageId="ENTER_DISCOUNT_HEADER" id="TitleLabel"/>
        <core:panel namingContainer="false" styleClass="numericInputPanel" id="NumericInputPanel">
            <core:elements/>
        </core:panel>
        <core:text messageId="#{notEmpty(messageId) ? messageId : 'ENTER_DISCOUNT_AMOUNT'}" id="NumericInputPanelPrompt"/>
        <core:button styleClass="numericKeypadEnterButton" id="EnterButton">
            <core:text>OKPressed</core:text>
            <core:labelText>ENT</core:labelText>
        </core:button>
        <core:inputText type="textField" focusOrder="1" id="NumericInputPanelInputBox">
            <core:text>${enteredDiscountAmount}</core:text>
            <core:textElementEvents enterEvent="OKPressed"/>
            <core:validationData>
                <core:currencyFieldValidationData>
                    <core:type>CurrencyAmount</core:type>
                    <core:inputMask></core:inputMask>
                    <core:validationClassname></core:validationClassname>
                    <core:maxValue>99999999</core:maxValue>
                    <core:currencyId>#{!empty(posTerminal.baseCurrencyId) and !empty(posTerminal.baseCurrencyId.currencyId) ? posTerminal.baseCurrencyId.currencyId : !empty(location.baseCurrencyKey) and !empty(location.baseCurrencyKey.currencyId) ? location.baseCurrencyKey.currencyId : ''}</core:currencyId>
                </core:currencyFieldValidationData>
            </core:validationData>
        </core:inputText>
        <core:panel namingContainer="false" id="FooterPanel">
            <core:elements>
                <core:pageElementRef pageDefinitionURL="Pos/FooterButtons" elementIdRef="FooterRefPanel" transparentBackground="true" id="FOOTER_BUTTONS_PAGE_REFERENCE">
                    <core:boundsConstraint height="87" width="480" y="0" x="0"/>
                    <core:elements/>
                    <core:parameter value="BAG" name="enactor.pos.DisplayHint">
                        <core:interfaceName>java.lang.String</core:interfaceName>
                    </core:parameter>
                </core:pageElementRef>
            </core:elements>
        </core:panel>
    </core:elements>
    <core:templateURL>NextGen480x800/Pos/FullScreenTraditionalPosTemplate</core:templateURL>
    <core:variables>
        <core:variable required="true" name="enactor.pos.enteredDiscountAmount">
            <core:interfaceName>java.lang.Long</core:interfaceName>
        </core:variable>
        <core:variable required="true" name="enactor.mfc.Location">
            <core:interfaceName>com.enactor.mfc.location.ILocation</core:interfaceName>
        </core:variable>
        <core:variable required="true" name="enactor.mfc.PosTerminal">
            <core:interfaceName>com.enactor.mfc.posTerminal.IPOSTerminal</core:interfaceName>
        </core:variable>
    </core:variables>
    <core:scannerEnabled>false</core:scannerEnabled>
    <core:scannerDataName></core:scannerDataName>
    <core:mSREnabled>false</core:mSREnabled>
    <core:mSRDataName></core:mSRDataName>
</retail:posPromptPage>
