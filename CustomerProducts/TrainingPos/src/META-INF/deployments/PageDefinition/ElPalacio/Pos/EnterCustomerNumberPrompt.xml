<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<retail:posPromptPage windowtype="view-panel" url="ElPalacio/Pos/EnterCustomerNumberPrompt" xmlns:core="http://www.enactor.com/core" xmlns:ns4="http://maven.apache.org/POM/4.0.0" xmlns:ns5="http://www.w3.org/2005/08/addressing" xmlns:ns6="http://schemas.xmlsoap.org/soap/envelope/" xmlns:ns7="http://www.w3.org/2003/05/soap-envelope" xmlns:ns9="tools" xmlns:retail="http://www.enactor.com/retail" xmlns:sref="http://docs.oasis-open.org/wsbpel/2.0/serviceref" xmlns:tools="http://www.enactor.com/tools">
    <core:elements>
        <core:inputText id="InputBox">
            <core:text>#{customerNumber}</core:text>
        </core:inputText>
        <core:button id="Button1">
            <core:text>OKPressed</core:text>
            <core:labelText>OK</core:labelText>
        </core:button>
        <core:button id="Button4">
            <core:text>CancelPressed</core:text>
            <core:labelText>Cancel</core:labelText>
        </core:button>
        <core:text id="Prompt">
            <core:text>Enter a Customer Number</core:text>
        </core:text>
    </core:elements>
    <core:templateURL>NextGen1024x768/Pos/FullScreenTraditionalPosTemplate</core:templateURL>
    <core:variables>
        <core:variable required="true" name="enactor.training.CustomerNumber">
            <core:interfaceName>java.lang.String</core:interfaceName>
        </core:variable>
    </core:variables>
</retail:posPromptPage>
